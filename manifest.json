{
  "manifest_version": 2,

  "name": "Sneaky Reader",
  "description": "Don't let your boss know!!",
  "version": "1.0",

  "browser_action": {},

  "permissions": [
    "tabs",
    "storage",
    "cookies",
    "*://takaogirl5566.com/"
  ],

  "content_scripts": [{
	"matches": ["<all_urls>"],
	"exclude_matches": ["https://www.facebook.com/*"],
	"css": ["mystyle.css"],
	"js": ["myscript.js"]
  }],
  
  "background": {
  	"scripts": ["background.js"]
  }
}
